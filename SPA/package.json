    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "2.2.1",
    "@uiw/react-color-wheel": "^2.3.0",
    "ag-grid-community": "31.1.1",
    "ag-grid-enterprise": "31.1.1",
    "ag-grid-react": "31.1.1",
    "customize-cra": "1.0.0",
    "framer-motion": "^11.3.31",
    "html2pdf.js": "^0.10.2",
    "lodash": "4.17.21",
    "lucide-react": "^0.378.0",
    "moment": "2.30.1",
    "mustache": "4.2.0",
    "react": "18.2.0",
    "react-app-rewired": "2.2.1",
    "react-calendar": "4.8.0",
    "react-dom": "18.2.0",
    "react-lottie": "^1.2.4",
    "react-redux": "9.1.0",
    "react-router": "6.22.2",
    "react-router-dom": "6.22.2",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.0",
    "react-top-loading-bar": "^2.3.1",
    "snake-camel": "1.0.8",
    "vite-plugin-svgr": "^4.2.0",
    "web-vitals": "3.5.2"
  },
  "scripts": {
    "build": "vite build",
    "start": "vite serve",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "react-refresh"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "react-refresh/only-export-components": "warn",
      "space-in-parens": [
        "error",
        "never"
      ],
      "no-multiple-empty-lines": "warn",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "no-use-before-define": "error",
      "indent": "off",
      "semi": [
        "warn",
        "never"
      ],
      "@typescript-eslint/no-explicit-any": "error",
      "keyword-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "no-trailing-spaces": "error",
      "import/no-duplicates": "error",
      "quotes": [
        "warn",
        "single",
        {
          "avoidEscape": false
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "20.11.24",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.18",
    "axios": "1.6.7",
    "classnames": "2.5.1",
    "css-loader": "6.10.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "postcss": "8.4.35",
    "postcss-loader": "8.1.1",
    "precss": "4.0.0",
    "recharts": "2.12.2",
    "style-loader": "3.3.4",
    "stylus": "0.62.0",
    "stylus-loader": "8.1.0",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "vite-plugin-simple-html": "0.1.2"
  },
  "overrides": {
    "typescript": "5.3.3"
  }
}
