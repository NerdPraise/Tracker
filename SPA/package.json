{
  "name": "tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "ag-grid-community": "^26.2.1",
    "ag-grid-enterprise": "^26.2.1",
    "ag-grid-react": "^26.2.0",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "customize-cra": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "months-list": "^1.0.3",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.11",
    "react-calendar": "^3.6.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "recharts": "^2.1.8",
    "snake-camel": "^1.0.8",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts  test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "space-in-parens": [
        "error",
        "never"
      ],
      "no-multiple-empty-lines": "warn",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "no-use-before-define": "error",
      "indent": [
        "warn",
        2
      ],
      "semi": [
        "warn",
        "never"
      ],
      "keyword-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "no-trailing-spaces": "error",
      "import/no-duplicates": "error",
      "quotes": [
        "warn",
        "single",
        {
          "avoidEscape": false
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "css-loader": "^6.5.1",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "precss": "^4.0.0",
    "style-loader": "^3.3.1",
    "stylus": "^0.56.0",
    "stylus-loader": "^6.2.0"
  }
}
