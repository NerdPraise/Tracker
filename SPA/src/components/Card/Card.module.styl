:local(.CardContainer)
  border-radius 10px
  border 1px solid $content-sidebar-gray
  background $content-sidebar-gray
  display flex
  width 100%
  height 100%
  flex-direction column
  :global(.children)
    padding 0 20px
    height 100%

:local(.cardHeader)
  display flex
  justify-content space-between
  padding 20px 20px 10px 20px
